    "kind": "dev",
    "methods": {
      "getCurrentLevel(address)": {
        "details": "Returns the \"level\" of the account, for systems using tiered representation."
      },
      "getCurrentVotes(address)": {
        "details": "Returns the number of votes allocated to the indicated account. For voting tokens this may be equal to {balanceOf} but is often a combination of balance, delegated votes, and other conditions."
      },
      "getPriorLevel(address,uint256)": {
        "details": "Returns the \"level\" of the account at the indicated block, which must be before the current block."
      },
      "getPriorVotes(address,uint256)": {
        "details": "Returns the number of votes allocated to the indicated account at the indicated block, which must be before the current block."
      },
      "totalVotes()": {
        "details": "Returns the total number of votes held by users. For voting tokens this is often equal to {totalSupply} but not always."
      }
    },
    "version": 1
  },
